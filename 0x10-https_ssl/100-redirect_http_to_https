frontend lb-01
    bind *:80 transparent
    bind *:443 ssl crt /home/ubuntu/haproxy.pem
    http-request redirect scheme https code 301 unless { ssl_fc }
    mode http
    default_backend webservers

backend webservers
    mode http
    balance roundrobin
    server 435189-web-01 54.88.119.71
    server 435189-web-02 54.237.98.230
